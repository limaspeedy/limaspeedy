end

local function createMenuButton(name)
	-- Instances:


	local MenuButton = Instance.new("TextButton")
	local UICorner = Instance.new("UICorner")

	--Properties:

	MenuButton.Name = tostring(menusCount)..name
	print(MenuButton.Name)
	MenuButton.Parent = GButtons
	MenuButton.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
	MenuButton.Position = UDim2.new(0.0382463485, 0, 0, 0)
	MenuButton.Size = UDim2.new(0.943000019, 0, 0.0329999998, 0)
	MenuButton.Font = Enum.Font.DenkOne
	MenuButton.Text = name
	MenuButton.TextColor3 = Color3.fromRGB(255, 255, 255)
	MenuButton.TextScaled = true
	MenuButton.TextSize = 14.000
	MenuButton.TextWrapped = true

	UICorner.Parent = MenuButton

	UICorner.Parent = MenuButton
	return MenuButton
end

