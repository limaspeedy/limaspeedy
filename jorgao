-- Script

local menusCount = 0
local menusFrame = GButtons
local afkActive = false
local G = getgenv()
G.Settings = {

};

-- Menu Creation

local function createMenuFrame(name)
	local MenuFrame = Instance.new("Frame")
	local UICorner = Instance.new("UICorner")
	local MenuTitle = Instance.new("TextLabel")
	local ScrollingFrame = Instance.new("ScrollingFrame")
	local UIListLayout = Instance.new("UIListLayout")
	local HelpFrame = Instance.new("Frame")
	local UICorner_2 = Instance.new("UICorner")
	local HelpText = Instance.new("TextLabel")

	--Properties:

	MenuFrame.Name = name
	MenuFrame.Parent = Frame
	MenuFrame.BackgroundColor3 = Color3.fromRGB(52, 52, 52)
	MenuFrame.Position = UDim2.new(0.273959696, 0, 0.0941176489, 0)
	MenuFrame.Size = UDim2.new(0.707664907, 0, 0.871586502, 0)
	MenuFrame.Visible = false

	UICorner.Parent = MenuFrame

	MenuTitle.Name = "MenuTitle"
	MenuTitle.Parent = MenuFrame
	MenuTitle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	MenuTitle.BackgroundTransparency = 1.000
	MenuTitle.Position = UDim2.new(0.0234394539, 0, -0.0937467813, 0)
	MenuTitle.Size = UDim2.new(0.934218109, 0, 0.188233122, 0)
	MenuTitle.Font = Enum.Font.Unknown
	MenuTitle.Text = name
	MenuTitle.TextColor3 = Color3.fromRGB(255, 255, 255)
