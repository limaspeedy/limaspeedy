-- Menu Utilities

local function offMenus()
	for _,v in pairs(Frame:GetChildren()) do
		if v:IsA("Frame") and v.Name ~= "Warn" and v.Name ~= "LE" then
			v.Visible = false
		end
	end
	for _,v in pairs(GButtons:GetChildren()) do
		if v:IsA("TextButton") then
			v.BackgroundColor3 = Color3.fromRGB(126, 126, 126)
		end
	end
end

local NameSave = game.Players.LocalPlayer.Name.."LDS"

local function LoadSettings()
	local HttpService = game:service('HttpService')

	--use 'isfile' to check if we are reading the correct txt file

	if isfile(NameSave) then
		--use 'readfile' to read 'TUTORIAL.txt' and decode it
		G.Settings = HttpService:JSONDecode(readfile(NameSave))
	end
end

local function SaveSettings()
	local JSON -- is nil
	local HttpService = game:service('HttpService') -- used for the JSON DECODE AND ENCODE
	--Decode
	JSON = HttpService:JSONEncode(G.Settings)
	--Make a text file using 'writefile'
	writefile(NameSave,JSON)
end

-- Warns

function module:ConfirmWarn(callback)
	Warn.Visible = true
	Dwarn.Visible = true
	Yes.MouseButton1Click:Connect(function()
		Warn.Visible = false
		Dwarn.Visible = false
		callback()
	end)
	No.MouseButton1Click:Connect(function()
		Warn.Visible = false
		Dwarn.Visible = false
		return false
	end)
end

function module:start(gameName, Hversion)
	GameName.Text = gameName
	HubVersion.Text = "v. "..Hversion
	UpdatesVTXT.Text = "v. "..Hversion
	global.Enabled = true
	game.StarterGui:SetCore("SendNotification", {
		Title = "LDS HUB";
		Text = "By Sr LDS and Aliscreative";
		Duration = 8;
	})
	local frame = global.Frame
	frame.HomeUi.Visible = true
	
	-- Funções principais da gui global
	_1Home.MouseButton1Click:Connect(function()
		offMenus()
		_1Home.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
		HomeUi.Visible = true
	end)
	_998Settings.MouseButton1Click:Connect(function()
		offMenus()
		_998Settings.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
		SettingsUI.Visible = true
	end)
	_999UpdateNotes.MouseButton1Click:Connect(function()
		offMenus()
		_999UpdateNotes.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
		UpdateNoteUi.Visible = true
	end)
		-- Créditos botões
	TextButtonYT.MouseButton1Click:Connect(function()
		setclipboard("https://www.youtube.com/@Sr_LDS")
		game.StarterGui:SetCore("SendNotification", {
			Title = "Youtube";
			Text = "Copied! Paste in your browser!";
			Duration = 6;
		})
	end)
	TextButtonDC.MouseButton1Click:Connect(function()
		setclipboard("https://discord.gg/srlds")
		game.StarterGui:SetCore("SendNotification", {
			Title = "Discord";
			Text = "Copied! Paste in your browser!";
			Duration = 6;
		})
	end)
		-- Settings botões
	AntAFKButton.MouseButton1Click:Connect(function()
		afkActive = not afkActive
		local bb=game:service('VirtualUser')
		local afkFunc
		if afkActive then
			AntiAFKFrameColor.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
			afkFunc = game:service('Players').LocalPlayer.Idled:connect(function()
				bb:CaptureController()
				bb:ClickButton2(Vector2.new())
			end)
		else
			AntiAFKFrameColor.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
			afkFunc:Disconnect()
			afkFunc = nil
		end
	end)
	
	SaveButton.MouseButton1Click:Connect(function()
		-- Em Desenvolvimento
	end)
	
	DeleteButton.MouseButton1Click:Connect(function()
		local function dstroy() global:Destroy() end
		module:ConfirmWarn(dstroy)
	end)
	
	-- Hide Button
	MenuH.TextButton.MouseButton1Click:Connect(function()
		Frame.Visible = not Frame.Visible
	end)
	
	-- Creating menu options
	local window = {}
	
	function window:addTab(name)
		menusCount += 1
		local cloneFrame = createMenuFrame(name)

		local cloneBt = createMenuButton(name)
		cloneBt.MouseButton1Click:Connect(function()
			offMenus()
			cloneBt.BackgroundColor3 = Color3.fromRGB(170, 0, 255)
			cloneFrame.Visible = true
		end)
		
		local tab = {}
		
		tab.MenuFrame = cloneFrame
		
		function tab:addToggle(text, description,  callback)
			local togBt, modBt, helpIcon = createToggleButton(text, description, tab.MenuFrame)
			local value = false
			if description ~= nil then
				helpIcon.MouseEnter:Connect(function()
					cloneFrame.HelpFrame.HelpText.Text = description
					cloneFrame.HelpFrame.Visible = true
				end)
				helpIcon.MouseLeave:Connect(function()
					cloneFrame.HelpFrame.Visible = false
				end)
			end
			togBt.MouseButton1Click:Connect(function()
				value = not value
				if value == true then
					modBt.BackgroundColor3 = Color3.fromRGB(0, 255, 0)
					modBt:TweenPosition(UDim2.new(0.5, 0,0.064, 0))
				else
					modBt.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
					modBt:TweenPosition(UDim2.new(0.057, 0,0.064, 0))
				end
				callback(value)
			end)
		end
		
		function tab:addToggleScroller()
			 createToggleButton(tab.MenuFrame)
			print("fc chamada pae")
		end
		
		return tab
		
	end
	
	
	
	return window
	
end

return module
